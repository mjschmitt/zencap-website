# Environment Variables Template
# Copy this file to .env.local and fill in your values

# Google Analytics
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# Contact Form (if using a service like Formspree or Netlify Forms)
NEXT_PUBLIC_CONTACT_FORM_ENDPOINT=https://formspree.io/f/your-form-id

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://zencap.co
NEXT_PUBLIC_SITE_NAME=Zenith Capital Advisors

# Optional: Email Service (for contact form)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password 